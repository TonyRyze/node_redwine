<!-- S 公共导航 -->
<div class="g-nav fr">
  <ul>
    <% for(var i = 0;i < navList.length;i++) { %>
      <% if(pageName === 'home') { %>
        <% if(navList[i] === '首页'){ %>
          <li><a href="/"><%= navList[i] %></a></li>
        <% } else{ %>
          <li><a href="javascript:;"><%= navList[i] %></a></li>
        <% } %>
      <% } else { %>
        <% if(navList[i] === '精彩活动'){ %>
          <li><a href="/active"><%= navList[i] %></a></li>
        <% } else if(navList[i] === '葡萄酒'){ %>
          <li><a href="/prodlist"><%= navList[i] %></a></li>
        <% } else{ %>
           <li><a href="/">首页</a></li>
        <% } %>
      <% } %>
    <% } %>
  </ul>
</div>
<div class="g-logo fl">
  <h1>
  	<a href="/" class="logo-bd"><img src="/images/logo.png" alt="深圳市前海佰迈投资有限公司"></a>
  </h1>
</div>
