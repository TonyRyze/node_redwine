<div class="g-header-wrap">
  <div class="g-logo">
    <h1>
      <% if (isMobile) { %>
        <a href="/" class="logo-bd"><img src="/images/m-logo.png" alt="深圳市前海佰迈投资有限公司"></a>
      <% } else{%>
        <a href="/" class="logo-bd"><img src="/images/logo.png" alt="深圳市前海佰迈投资有限公司"></a>
      <%}%>
    </h1>
  </div>
  <div class="menu-ext">
    <div class="ext-icon">
      <i></i>
      <i></i>
      <i></i>
    </div>
    <div class="g-nav">
      <ul>
        <% for(var i = 0;i < navList.length;i++) { %>
          <% if(navList[i] === '首页'){ %>
            <li><a href="/"><%= navList[i] %></a></li>
          <% } else{ %>
            <li><a href="javascript:;"><%= navList[i] %></a></li>
          <% } %>
        <%}%>
      </ul>
    </div>
  </div>
</div>
